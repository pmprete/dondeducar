@* grid *@
<div id="gridEscuelas" class="grid"></div>
@* /grid *@

<script type="text/javascript">
    $(document).ready(function () {

        //>>>Ventas
        $.getJSON("@Url.Action("GetEscuelas")", { tagId: 1 }, function (data) {       
            $("#gridEscuelas").slickgrid({
                columns: EDUCAR.Grids.ColumnasEscuelas,
                data: data,
                slickGridOptions: EDUCAR.Grids.DefaultSlickGridOptions
            });//<<<slickgrid            
        });
    });//<<<ready
</script>